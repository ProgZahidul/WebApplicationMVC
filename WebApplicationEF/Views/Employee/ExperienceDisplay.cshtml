@model IEnumerable<WebApplicationEF.Models.Experience>
@{
    Layout = null;
}





<table class="table-active table-borderless table table-hover table-responsive" id="experienceTable">
    <thead class="table-info">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Designation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Comapany)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StartDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EndDate)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var e in Model)
        {
            <tr>
                <td class="form-group">
                    @Html.DisplayFor(m => e.Designation)
                </td>
                <td class="form-group">
                    @Html.DisplayFor(m => e.Comapany)
                </td>
                <td class="form-group">
                    @Html.DisplayFor(m => e.StartDate)
                </td>
                <td class="form-group">
                    @Html.DisplayFor(m => e.EndDate)
                </td>
            </tr>
        }
    </tbody>
</table>